<div class="profile">
  <div class="container">
     <h1>Profile Update</h1>
     <%= form_for(@user) do |f| %>
      <%= f.label :first_name %>
      <%= f.text_field :first_name %>

      <%= f.label :last_name %>
      <%= f.text_field :last_name %>

      <%= f.label :email %>
      <%= f.email_field :email %>
      <%= f.submit "Update", :class => "btn" %>
    <% end %> 
    
  </div>
</div> 